          "type": "boolean",
          "default": false,
          "title": "The racist Schema",
          "examples": [
            false
          ]
        },
        "sexist": {
          "type": "boolean",
          "default": false,
          "title": "The sexist Schema",
          "examples": [
            false
          ]
        },
        "explicit": {
          "type": "boolean",
          "default": false,
          "title": "The explicit Schema",
          "examples": [
            false
          ]
        }
      },
      "examples": [{
        "nsfw": false,
        "religious": false,
        "political": false,
        "racist": false,
        "sexist": false,
        "explicit": false
      }]
